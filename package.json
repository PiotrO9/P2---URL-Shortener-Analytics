{
	"name": "url-shortener",
	"version": "1.0.0",
	"description": "Prosta aplikacja API z Fastify",
	"main": "dist/app.js",
	"scripts": {
		"start": "node dist/app.js",
		"build": "tsc",
		"dev": "ts-node app.ts",
		"dev:watch": "ts-node --watch app.ts",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write .",
		"type-check": "tsc --noEmit",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"docker:up": "docker-compose up --build",
		"docker:down": "docker-compose down",
		"docker:clean": "docker-compose down -v"
	},
	"dependencies": {
		"@fastify/sensible": "^6.0.3",
		"@prisma/client": "^5.7.1",
		"crypto": "^1.0.1",
		"fastify": "^4.24.3",
		"prom-client": "^15.1.3",
		"redis": "^5.8.1"
	},
	"devDependencies": {
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.13.0",
		"@typescript-eslint/parser": "^6.13.0",
		"eslint": "^8.54.0",
		"prettier": "^3.1.0",
		"prisma": "^5.7.1",
		"ts-node": "^10.9.0",
		"typescript": "^5.3.0"
	},
	"keywords": [
		"fastify",
		"api",
		"nodejs"
	],
	"author": "",
	"license": "MIT"
}
